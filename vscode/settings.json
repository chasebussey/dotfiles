{
    "editor.minimap.enabled": false,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["Z", "Z"],
            "commands": [":wq"]
        },
        {
            "before": ["<leader>", "e"],
            "commands": ["workbench.action.toggleSidebarVisibility"]
        },
        {
            "before": ["<leader>", "f"],
            "commands": [ "revealInExplorer" ],
        },
        {
            "before": ["<leader>", "w"],
            "commands": [ ":w" ]
        },
        {
            "before": ["<leader>", "q"],
            "commands": [":q"]
        }
    ]
}